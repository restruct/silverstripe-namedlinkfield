{
  "name": "restruct/silverstripe-namedlinkfield",
  "description": "A SilverStripe composite field for having various link types *with title*, in a single field with a nicely styled form.",
  "type": "silverstripe-vendormodule",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Restruct web & apps",
      "email": "dev@restruct.nl"
    }
  ],
  "require": {
    "php": ">=7.4",
    "silverstripe/vendor-plugin": ">=1",
    "silverstripe/framework": "^4 || ^5",
    "sheadawson/silverstripe-dependentdropdownfield": "^3"
  },
  "autoload": {
    "psr-4": {
      "Restruct\\SilverStripe\\Forms\\": "src/",
      "Restruct\\SilverStripe\\ORM\\FieldType\\": "src/"
    }
  },
  "extra": {
    "installer-name": "namedlinkfield",
    "expose": [
      "client"
    ]
  }
}
